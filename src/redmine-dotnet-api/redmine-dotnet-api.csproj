<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <TargetFrameworks>net20;net35;net40;netstandard16;netstandard20;netstandard21;netcoreapp</TargetFrameworks>
        <RootNamespace>Redmine.dotNet.Api</RootNamespace>
        <ImplicitUsings>false</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup Label="Internals Visible To">
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
            <_Parameter1 Condition="'$(Sign)' == '' OR '$(Sign)' == 'false'">$(MSBuildProjectName).Tests</_Parameter1>
            <_Parameter1 Condition="'$(Sign)' == 'true'">$(MSBuildProjectName).Tests, 
            </_Parameter1>
        </AssemblyAttribute>
    </ItemGroup>

</Project>
